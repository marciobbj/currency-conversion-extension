{
    "manifest_version": 3,
    "name": "Currency Today",
    "description": "chrome extension",
    "version": "0.0.1",
    "background": {
        "service_worker": "/main.js"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ],
            "all_frames": true
        }
    ]
}