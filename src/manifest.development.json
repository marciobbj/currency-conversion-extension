{
    "name": "Currency Today",
    "description": "Get currency convertions instantaneously",
    "version": "0.0.1",
    "manifest_version": 3,
    "host_permissions": [
      "*://*/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["content.js"],
        "all_frames": true
      }
    ],
    "action": {},
    "permissions": [
        "activeTab",
        "scripting",
        "storage"
      ],
    "background": {
      "service_worker": "main.js"
    }
}